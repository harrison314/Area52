{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UfoSetup": {
    "Area52Endpoint": "https://localhost:5698/",
    "ApiKey": null,
    "RecomandetBatchSize": 500
  },
  "FolderWatchSetup": {
    "Enabled": false,
    "Folder": "./bin/SotrceLogs",
    "GlobPattern": "*.log",
    "TmpFolder": "./bin/tmp"
  },
  "SyslogSetup": {
    "Enabled": false,
    "Protocol": "Udp",
    "Host": "127.0.0.1",
    "Port": 312,
    "AdditionalPropertys": [
      {
        "Name": "IsSyslog",
        "Value": true,
        "Override": true
      }
    ]
  }
}
